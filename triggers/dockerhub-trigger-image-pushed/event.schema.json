{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "pushedat": {
        "type": "string",
        "description": "Time image was last pushed",
        "format": "date-time"
      },
      "pusher": {
        "type": "integer",
        "description": "Image pusher"
      },
      "tag": {
        "type": "string",
        "description": "Image tag"
      },
      "repoName": {
        "type": "string",
        "description": "Image repository name"
      },
      "description": {
        "type": "string",
        "description": "Image repository description"
      },
      "fullDescription": {
        "type": "string",
        "description": "Image repository full description"
      },
      "owner": {
        "type": "string",
        "description": "Image repository owner"
      },
      "isOfficial": {
        "type": "boolean",
        "description": "Whether repository is official"
      },
      "isPrivate": {
        "type": "boolean",
        "description": "Whether repository is private"
      },
      "namespace": {
        "type": "string",
        "description": "Image repository namespace"
      },
      "repoUrl": {
        "type": "string",
        "description": "Image repository URL"
      }
    },
    "required": [
      "pushedat",
      "pusher",
      "tag",
      "repoName",
      "description",
      "fullDescription",
      "owner",
      "isOfficial",
      "isPrivate",
      "namespace",
      "repoUrl"
    ],
    "additionalProperties": false
  }