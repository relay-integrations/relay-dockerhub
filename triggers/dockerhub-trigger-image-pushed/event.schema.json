{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "pushedat": {
        "type": "string",
        "description": "Time image was last pushed",
        "format": "date-time"
      },
      "pusher": {
        "type": "integer",
        "description": "Image pusher"
      },
      "tag": {
        "type": "string",
        "description": "Image tag"
      },
      "name": {
        "type": "string",
        "description": "Image repository name"
      }
    },
    "required": [
      "pushedat",
      "pusher",
      "tag",
      "name"
    ],
    "additionalProperties": false
  }